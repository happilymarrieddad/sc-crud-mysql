{
  "name": "mvc-example",
  "description": "A sample Vue-Socketcluster app",
  "version": "1.1.0",
  "contributors": [
    {
      "name": "Nick Kotenberg",
      "email": "nkotenberg@happilymarrieddad@yahoo.com"
